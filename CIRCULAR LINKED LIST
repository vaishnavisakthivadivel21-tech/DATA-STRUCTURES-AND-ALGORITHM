#include <stdio.h>
#include<stdlib.h>

struct node{
    int data;
    struct node *next;
};
struct node *head=NULL;
void append(int data){ //30
    struct node*newnode=malloc(sizeof(struct node));
    newnode->data=data;
 if(head==NULL){
     head=newnode;  //circular changes
     newnode->next=head;
 }   
 else{
     struct node*temp=head; 
     while(temp->next!=head){
         temp=temp->next; 
     }
     temp->next=newnode;
     newnode->next=head; //circluar
 } 
 
}
void display(){  //dowhile
  struct node *temp=head;
  do{
      printf("%d ",temp->data);
      temp=temp->next;
  }
  while(temp!=head);
}
int main()
{
    int n;
    int data;
    printf("enter number of node:");
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        scanf("%d",&data);
        if(data>0){
            append(data);
        }
    }
    printf("the data in the nodes are:");
    display();
}
   
