#include <stdio.h>

#define MAX 5
int queue[MAX];
int front=0,rear=-1;
int main()
{
    int n,value;
    printf("enter number of elements to insert(max %d):",MAX);
    scanf("%d",&n);
    if(n>MAX){
        printf("overflow");
        return 0;
    }
    
    for(int i=0;i<n;i++){
        scanf("%d",&queue[i]);
        rear++;
    }
    if(front>rear){
        printf("underflow");
    }
    else{
        printf("deleted element:%d ",queue[front]);
        front++;
    }
    printf("\nqueue after deqeue:\n");
     if(front>rear){
        printf("empty");
     }else{
    for(int i=front;i<=rear;i++){
        printf("%d ",queue[i]);
    }
     }
    return 0;
}
